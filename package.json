{
  "name": "@berlysia/react-virtual-scroll",
  "version": "0.1.1",
  "description": "height cacheable virtual scroll for react",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/",
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present changelog:prepublish && npm run --if-present build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:prepublish": "npm run changelog && git add CHANGELOG.md && git commit -m 'docs(CHANGELOG): Update on prepublish'"
  },
  "author": "berlysia",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-auto-import-assert": "^1.0.3",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-unassert": "^2.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.5.0",
    "power-assert": "^1.3.1",
    "react-addons-test-utils": "^15.0.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "map-like": "^1.0.1",
    "request-frame": "^1.4.1",
    "virtual-scroll-core": "^0.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berlysia/react-virtual-scroll.git"
  },
  "bugs": {
    "url": "https://github.com/berlysia/react-virtual-scroll/issues"
  },
  "homepage": "https://github.com/berlysia/react-virtual-scroll#readme"
}
